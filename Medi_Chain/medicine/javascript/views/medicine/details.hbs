

{{#each products  }}
    <div class="row">
        
            <div class="col-sm-6 col-md-4">
                <div class="thumbnail">
                    <img src="{{ this.Img_path }}" alt="..." class="img-responsive">
                    <div class="caption">
                        <h3>{{ this.Name }}</h3>
                        <p class="description">Company: {{ this.PCompany }}</p>
                        <p class="description">Current Stage: {{ this.Currstage }}</p>
                        <div class="clearfix">
                            
                          <!-- <a href="/view_details/{{this._id}}" class="btn btn-success pull-right" role="button">View Details</a>-->
                        <a href="/" class="btn btn-success pull-right" role="button">Back to Home</a>
                        </div>
                    </div>
                </div>
            </div>
      
    </div>
{{/each}}