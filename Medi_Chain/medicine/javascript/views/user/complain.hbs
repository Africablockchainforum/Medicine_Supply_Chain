<center>

    <h3>{{title}}</h3>
</center>
{{#if message}}
<div class="alert alert-info">

    <p>{{this.message}}</p>

</div>
{{/if}}

<div>
    {{#each complain  }}

    <li>
        <h4><i class="fa fa-database">

                complain from {{ this.CName }}({{this.CId}}): </i></h4><br />
        <p class="description">{{ this.Complain }}</p>
        <h6>{{this.Time}}</h6>

    </li>
    {{/each}}
    <br />
    <br />
</div>
<a href="/" class="btn btn-success pull-right" role="button">Back to Home</a>